{
  "name": "@veramo/sd-jwt",
  "description": "Veramo plugin to enable SD-JWT",
  "version": "5.6.0",
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js"
  },
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "generate-plugin-schema": "node ../cli/bin/veramo.js dev generate-plugin-schema"
