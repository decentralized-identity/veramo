{
  "name": "@veramo/core",
  "description": "Veramo Core Logic & Interfaces.",
  "version": "5.6.0",
  "main": "build/index.cjs.js",  
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs.js"
    }
  },
  "scripts": {
    "build": "node ../../scripts/esbuild.js",
    "extract-api": "node ../cli/bin/veramo.js dev extract-api"
  },
  "dependencies": {
    "@veramo/core-types": "workspace:^",
    "debug": "^4.3.4",
    "events": "^3.2.0",
    "z-schema": "^6.0.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.8",
    "@types/events": "3.0.0",
    "typescript": "5.2.2"
  },
  "files": [
    "build/**/*",
    "src/**/*",
    "plugin.schema.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentralized-identity/veramo.git",
    "directory": "packages/core"
  },
  "author": "Consensys Mesh R&D <hello@veramo.io>",
  "contributors": [
    "Simonas Karuzas <simonas.karuzas@mesh.xyz>",
    "Mircea Nistor <mircea.nistor@mesh.xyz>"
  ],
  "keywords": [
    "Veramo",
    "DID",
    "Verifiable Credential",
    "plugin host"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
